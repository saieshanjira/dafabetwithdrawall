 <\/span>\r\n        <\/div>\r\n    <\/a>\r\n<\/div>\r\n\r\n<style>\r\n    .frosmo-leaderboard-banner-wrapper {\r\n        margin: 0 10px;\r\n    }\r\n    .frosmo-leaderboard-banner-link, \r\n    .frosmo-leaderboard-banner {\r\n        display: none;\r\n        \/*width: 1300px;*\/\r\n        height: 160px;\r\n        margin: 0 auto;\r\n        position: relative;\r\n        left: -8px;\r\n    }\r\n    .frosmo-leaderboard-banner-link > a {\r\n        display: block;\r\n        width: 100%;\r\n        height: 100%;\r\n    }\r\n    .frosmo-leaderboard-banner-close {\r\n        font-weight: bold;\r\n        color: #fff;\r\n        background: none;\r\n        font-size: 20px;\r\n        width: 32px;\r\n        height: 32px;\r\n        font-size: 28px;\r\n        padding: 0;\r\n        float: right;\r\n        z-index: 100;\r\n    }\r\n    .frosmo-leaderboard-banner-close:focus {\r\n        border: none;\r\n        outline: none;\r\n    }\r\n    .frosmo-banner-text-wrapper {\r\n        text-align: center;\r\n        position: absolute;\r\n    }\r\n    .frosmo-leaderboard-banner-title,\r\n    .frosmo-leaderboard-banner-description {\r\n        text-align: center;\r\n    }\r\n    .frosmo-leaderboard-banner-title {\r\n        color: #ffe000;\r\n        font-size: 42px;\r\n    }\r\n    .frosmo-leaderboard-banner-description {\r\n        color: #fff;\r\n        font-size: 28px;\r\n    }\r\n<\/style>\r\n\r\n<script>\r\n    var isBannerAlreadyShown = false;\r\n    var expectedOrigin = window.location.protocol + '\/\/' + 'als.' + window.location.hostname.replace('www.', '');\r\n    window.addEventListener('message', _receiveMessage, expectedOrigin);\r\n\r\n    \r\n    function _receiveMessage(event) {\r\n        if (event.origin !== expectedOrigin || !event.data || !event.data.frosmo) {\r\n            return;\r\n        }\r\n\r\n        if (event.data.name === 'iframe-width') {\r\n            if (event.data.content) {\r\n                _setBannerWidth(event.data.content);\r\n            }\r\n        }\r\n        if (!isBannerAlreadyShown) {\r\n            _addUserParameterToLink();\r\n            _applyBannerType();\r\n            _closeBannerHandler();\r\n            _addStyles();\r\n            isBannerAlreadyShown = true;\r\n        }\r\n    }\r\n\r\n    function _setBannerWidth(width) {\r\n        var bannerLink = document.querySelector('.frosmo-leaderboard-banner-link');\r\n        var banner = document.querySelector('.frosmo-leaderboard-banner');\r\n        bannerLink.style.width = width;\r\n        banner.style.width = width;\r\n    }\r\n    \r\n    function _addUserParameterToLink() {\r\n        var linkEl = document.querySelector('.frosmo-leaderboard-banner-link a');\r\n        var navLinkEl = document.querySelector('.extra-menu > .extra-nav-promotions > a');\r\n        if (navLinkEl) {\r\n            var href = navLinkEl.getAttribute('href');\r\n            var isPromotion = href.indexOf('promotion') > -1;\r\n            if (isPromotion) {\r\n                var userParam = href.substring(href.indexOf(\"?\"));\r\n                var currentUrl = linkEl.getAttribute('href');\r\n                linkEl.setAttribute('href', currentUrl + userParam);\r\n            }\r\n        }\r\n    }\r\n    \r\n    function _applyBannerType() {\r\n        var type = '{{type}}';\r\n        var linkEl = document.querySelector('.frosmo-leaderboard-banner-link a');\r\n        if (type === 'popup') {\r\n            linkEl.addEventListener('click', function (e) {\r\n                e.preventDefault();\r\n                window.open(this.href, '{{name}}', 'width=1000,height=500,left=100,top=100, location=0');\r\n            });\r\n        } else {\r\n            linkEl.setAttribute('target', '_blank');\r\n        }\r\n    }\r\n    \r\n    function _closeBannerHandler() {\r\n        var bannerSelector = _getBannerSelector();\r\n        var closeButtonSelector = bannerSelector + ' > .frosmo-leaderboard-banner-close';\r\n        var bannerElement = document.querySelector(bannerSelector);\r\n        var closeButtonElement = document.querySelector(closeButtonSelector);\r\n        \r\n        closeButtonElement.addEventListener('click', function () {\r\n            \r\n            \/\/ hiding banner\r\n            bannerElement.style.display = 'none';\r\n            \r\n            \/\/ reverting original navbar margin back to default\r\n            document.querySelector('.product-navigation-bar').style.marginBottom = '10px';\r\n            \r\n            \/\/ record to context to not show again\r\n            if (!frosmo.easy.context.site.dafa261Closed) {\r\n                frosmo.easy.context.site.dafa261Closed = [];\r\n            }\r\n            frosmo.easy.context.site.dafa261Closed.push('{{name}}');\r\n            frosmo.easy.context.save();\r\n        });\r\n    }\r\n    \r\n    function _addStyles() {\r\n        var bannerSelector = _getBannerSelector();\r\n        \r\n        _updateBackground();\r\n        _addBannerImage(bannerSelector);\r\n        _applyTextStyle(bannerSelector);\r\n        _applyBannerMargin(bannerSelector);\r\n        _applyCloseButtonColor(bannerSelector);\r\n        \r\n        function _updateBackground() {\r\n            var background = '{{& background}}';\r\n            if (background) {\r\n                document.body.style.background = 'url(\"' + background + '\") center top repeat fixed';\r\n            }\r\n        }\r\n        \r\n        function _addBannerImage(bannerSelector) {\r\n            var bannerImage = '{{& banner-image}}';\r\n            if (bannerImage) {\r\n                var bannerEl = document.querySelector(bannerSelector);\r\n                bannerEl.style.display = 'block';\r\n                bannerEl.style.background = 'url(\"' + bannerImage + '\") center center no-repeat';\r\n                bannerEl.style.backgroundSize = 'cover';\r\n                \r\n                var bannerHeight = '{{banner-height}}';\r\n                if (bannerHeight) {\r\n                    bannerEl.style.height = bannerHeight;\r\n                }\r\n                \/*\r\n                var bannerWidth = '{{banner-width}}';\r\n                if (bannerWidth) {\r\n                    bannerEl.style.width = bannerWidth;\r\n                }*\/\r\n                \r\n                \/\/ removing original navbar margin\r\n                document.querySelector('.product-navigation-bar').style.marginBottom = '0px';\r\n            }\r\n        }\r\n        \r\n        function _applyTextStyle(bannerSelector) {\r\n            var bannerEl = document.querySelector(bannerSelector);\r\n            var textWrapperEl = bannerEl.querySelector('.frosmo-banner-text-wrapper');\r\n            var titleEl = textWrapperEl.querySelector('.frosmo-leaderboard-banner-title');\r\n            var descriptionEl = textWrapperEl.querySelector('.frosmo-leaderboard-banner-description');\r\n            \r\n            \/\/ position\r\n            var parentHeight = bannerEl.offsetHeight;\r\n            var parentWidth = bannerEl.offsetWidth;\r\n            var childHeight = textWrapperEl.offsetHeight;\r\n            var childWidth = textWrapperEl.offsetWidth;\r\n            textWrapperEl.style.top = (parentHeight - childHeight) \/ 2 + 'px';\r\n            textWrapperEl.style.left = (parentWidth - childWidth) \/ 2 + 'px';\r\n            \r\n            \/\/ color\r\n            var titleColor = '{{banner-title-color}}';\r\n            var descriptionColor = '{{banner-description-color}}';\r\n            if (titleColor) {\r\n                titleEl.style.color = titleColor;\r\n            }\r\n            if (descriptionColor) {\r\n                descriptionEl.style.color = descriptionColor;\r\n            }\r\n            \r\n            \/\/ size\r\n            var titleSize = '{{banner-title-size}}';\r\n            var descriptionSize = '{{banner-description-size}}';\r\n            if (titleSize) {\r\n                titleEl.style.fontSize = titleSize;\r\n            }\r\n            if (descriptionSize) {\r\n                descriptionEl.style.fontSize = descriptionSize;\r\n            }\r\n        }\r\n        \r\n        function _applyBannerMargin(bannerSelector) {\r\n            var bannerEl = document.querySelector(bannerSelector);\r\n            var bannerTopMargin = '{{banner-top-margin}}';\r\n            var bannerBottomMargin = '{{banner-bottom-margin}}';\r\n            \r\n            if (bannerTopMargin) {\r\n                bannerEl.style.marginTop = bannerTopMargin;\r\n            }\r\n            if (bannerBottomMargin) {\r\n                bannerEl.style.marginBottom = bannerBottomMargin;\r\n            }\r\n        }\r\n        \r\n        function _applyCloseButtonColor(bannerSelector) {\r\n            var closeButtonColor = '{{banner-close-button-color}}';\r\n            var bannerEl = document.querySelector(bannerSelector);\r\n            var closeButtonEl = bannerEl.querySelector('.frosmo-leaderboard-banner-close');\r\n            \r\n            if (closeButtonColor) {\r\n                closeButtonEl.style.color = closeButtonColor;\r\n            }\r\n        }\r\n    }\r\n    function _getBannerSelector() {\r\n        var bannerSelector = '.frosmo-leaderboard-banner';\r\n        \r\n        if (_isLink()) {\r\n            bannerSelector = '.frosmo-leaderboard-banner-link';\r\n        }\r\n        \r\n        return bannerSelector;\r\n    }\r\n    function _isLink() {\r\n        return '{{{banner-link}}}';\r\n    }\r\n<\/script>","dafa-271 - Dafa Sports - Livestream banner":"<div class=\"frosmo-leaderboard-banner-iframe\">\r\n    <button class=\"frosmo-leaderboard-banner-iframe-close\">\u00d7<\/button>\r\n    <div class=\"frosmo-banner-text-wrapper\">\r\n        <div class=\"frosmo-leaderboard-banner-iframe-title\">\r\n            {{banner-title-text}}\r\n        <\/div>\r\n        <div class=\"frosmo-leaderboard-banner-iframe-description\">\r\n            {{banner-description-text}}\r\n        <\/div>\r\n    <\/div>\r\n<\/div>\r\n<div class=\"frosmo-leaderboard-banner-iframe-link\">\r\n    <button class=\"frosmo-leaderboard-banner-iframe-close\">\u00d7<\/button>\r\n    <a href=\"{{banner-link}}\">\r\n        <div class=\"frosmo-banner-text-wrapper\">\r\n            <span class=\"frosmo-leaderboard-banner-iframe-title\">\r\n                {{banner-title-text}}\r\n            <\/span>\r\n            <br\/>\r\n            <span class=\"frosmo-leaderboard-banner-iframe-description\">\r\n                {{banner-description-text}}\r\n            <\/span>\r\n        <\/div>\r\n    <\/a>\r\n<\/div>\r\n\r\n<style>\r\n    .frosmo-leaderboard-banner-iframe-link, \r\n    .frosmo-leaderboard-banner-iframe {\r\n        display: none;\r\n        \/*width: 1300px;*\/\r\n        height: 160px;\r\n        margin: 0 auto;\r\n        position: relative;\r\n    }\r\n    .frosmo-leaderboard-banner-iframe-link > a {\r\n        display: block;\r\n        width: 100%;\r\n        height: 100%;\r\n    }\r\n    .frosmo-leaderboard-banner-iframe-close {\r\n        font-weight: bold;\r\n        color: #fff;\r\n        background: none;\r\n        width: 32px;\r\n        height: 32px;\r\n        font-size: 28px;\r\n        padding: 0;\r\n        float: right;\r\n        z-index: 100;\r\n        border: 0;\r\n        cursor: pointer;\r\n        vertical-align: middle;\r\n        font-family: \"Trebuchet MS\",Arial,sans-serif;\r\n    }\r\n    .frosmo-leaderboard-banner-iframe-close:focus {\r\n        border: none;\r\n        outline: none;\r\n    }\r\n    .frosmo-banner-text-wrapper {\r\n        text-align: center;\r\n        position: absolute;\r\n        width: 100%;\r\n    }\r\n    .frosmo-leaderboard-banner-iframe-title,\r\n    .frosmo-leaderboard-banner-iframe-description {\r\n        text-align: center;\r\n    }\r\n    .frosmo-leaderboard-banner-iframe-title {\r\n        color: #ffe000;\r\n        font-size: 42px;\r\n    }\r\n    .frosmo-leaderboard-banner-iframe-description {\r\n        color: #fff;\r\n        font-size: 28px;\r\n    }\r\n    .secondary-header {\r\n        margin-top: 0 !important;\r\n    }\r\n<\/style>\r\n\r\n<script>\r\n    \/\/ set background from iframe\r\n    _sendDataFromIframeToParent('background', '{{& background}}');\r\n    _sendDataFromIframeToParent('referrerType', _getReferrerType());\r\n    \r\n    _setBannerWidth();\r\n\r\n    \/\/ keep adjusting the banner on window resizing \r\n    window.onresize = _setBannerWidth;\r\n\r\n    \/\/ banner link can open the popup or separate tab \r\n    \/\/ depending on template variables set up \r\n    _applyBannerType();\r\n    _closeBannerHandler();\r\n    _addStyles();\r\n    _fixationHandling();\r\n\r\n    \/\/ change original layout styles when banner will appear\r\n    _sendDataFromIframeToParent('banner-shown');\r\n\r\n    window.addEventListener('scroll', _fixationHandling);\r\n\r\n    \/\/ get the authorisation link for promotions\r\n    var expectedOrigin = window.location.protocol + '\/\/' + 'www.' + window.location.hostname.replace('als.', '');\r\n    window.addEventListener('message', _receiveMessage, expectedOrigin);\r\n    function _receiveMessage(event) {\r\n        if (event.origin !== expectedOrigin || !event.data || !event.data.frosmo) {\r\n            return;\r\n        }\r\n\r\n        switch(event.data.name) {\r\n            case 'authorization-parameter':\r\n                _addAuthorizationParameter(event.data.content);\r\n                break;\r\n            default:\r\n                break;\r\n        }\r\n    }\r\n    \r\n    function _setBannerWidth() {\r\n        var container = document.querySelector('#sub-header > .container');\r\n        var containerWidth = container.offsetWidth;\r\n        var bannerLink = document.querySelector('.frosmo-leaderboard-banner-iframe-link');\r\n        var banner = document.querySelector('.frosmo-leaderboard-banner-iframe');\r\n        bannerLink.style.width = containerWidth +'px';\r\n        banner.style.width = containerWidth +'px';\r\n    }\r\n    \r\n    function _applyBannerType() {\r\n        var type = '{{type}}';\r\n        var linkEl = document.querySelector('.frosmo-leaderboard-banner-iframe-link a');\r\n        if (type === 'popup') {\r\n            linkEl.addEventListener('click', function (e) {\r\n                e.preventDefault();\r\n                window.open(this.href, '{{name}}', 'width=1000,height=500,left=100,top=100, location=0');\r\n            });\r\n        } else {\r\n            linkEl.setAttribute('target', '_blank');\r\n        }\r\n    }\r\n    \r\n    function _closeBannerHandler() {\r\n        var bannerSelector = _getBannerSelector();\r\n        var closeButtonSelector = bannerSelector + ' > .frosmo-leaderboard-banner-iframe-close';\r\n        var bannerElement = document.querySelector(bannerSelector);\r\n        var closeButtonElement = document.querySelector(closeButtonSelector);\r\n        \r\n        closeButtonElement.addEventListener('click', function () {\r\n            \/\/ hiding banner\r\n            bannerElement.style.display = 'none';\r\n            \r\n            \/\/ reverting original navbar margin back to default\r\n            \/\/ document.querySelector('#header').style.top = '0';\r\n            document.querySelector('#header').style.position = 'fixed';\r\n\r\n            \/\/ adopt live section container\r\n            \/\/ 5 is default margin for container\r\n            \/\/ 30 is navbar's height\r\n            var defaultContainerMargin = 5;\r\n            var navbarEl = document.querySelector('#header');\r\n            var navbarHeight = navbarEl.offsetHeight;\r\n            if (document.querySelector('html.live #container')) {\r\n                document.querySelector('html.live #container').style.top = (defaultContainerMargin + navbarHeight + 'px');\r\n            }\r\n            \r\n            \/\/ record to context to not show again\r\n            if (!frosmo.easy.context.site.dafa261Closed) {\r\n                frosmo.easy.context.site.dafa261Closed = [];\r\n            }\r\n\r\n            \/\/ we need a name to hide only current banner so the future ones\r\n            \/\/ would avoid this check\r\n            frosmo.easy.context.site.dafa261Closed.push('{{name}}');\r\n            frosmo.easy.context.save();\r\n            \r\n            _sendDataFromIframeToParent('banner-closed');\r\n        });\r\n    }\r\n    \r\n    function _addStyles() {\r\n        var bannerSelector = _getBannerSelector();\r\n        \r\n        _addBannerImage(bannerSelector);\r\n        _applyTextStyle(bannerSelector);\r\n        _applyBannerMargin(bannerSelector);\r\n        _applyCloseButtonColor(bannerSelector);\r\n        \r\n        function _addBannerImage(bannerSelector) {\r\n            var bannerImage = '{{& banner-image}}';\r\n            if (bannerImage) {\r\n                var bannerEl = document.querySelector(bannerSelector);\r\n                bannerEl.style.display = 'block';\r\n                bannerEl.style.background = 'url(\"' + bannerImage + '\") center center no-repeat';\r\n                bannerEl.style.backgroundSize = 'cover';\r\n                \r\n                var bannerHeight = '{{banner-height}}';\r\n                if (bannerHeight) {\r\n                    bannerEl.style.height = bannerHeight;\r\n                }\r\n\r\n                document.querySelector('#header').style.position = 'relative';\r\n            }\r\n        }\r\n        \r\n        function _applyTextStyle(bannerSelector) {\r\n            var bannerEl = document.querySelector(bannerSelector);\r\n            var textWrapperEl = bannerEl.querySelector('.frosmo-banner-text-wrapper');\r\n            var titleEl = textWrapperEl.querySelector('.frosmo-leaderboard-banner-iframe-title');\r\n            var descriptionEl = textWrapperEl.querySelector('.frosmo-leaderboard-banner-iframe-description');\r\n            \r\n            \/\/ position\r\n            var parentHeight = bannerEl.offsetHeight;\r\n            var parentWidth = bannerEl.offsetWidth;\r\n            var childHeight = textWrapperEl.offsetHeight;\r\n            var childWidth = textWrapperEl.offsetWidth;\r\n            textWrapperEl.style.top = (parentHeight - childHeight) \/ 2 + 'px';\r\n            textWrapperEl.style.left = (parentWidth - childWidth) \/ 2 + 'px';\r\n            \r\n            \/\/ color\r\n            var titleColor = '{{banner-title-color}}';\r\n            var descriptionColor = '{{banner-description-color}}';\r\n            if (titleColor) {\r\n                titleEl.style.color = titleColor;\r\n            }\r\n            if (descriptionColor) {\r\n                descriptionEl.style.color = descriptionColor;\r\n            }\r\n            \r\n            \/\/ size\r\n            var titleSize = '{{banner-title-size}}';\r\n            var descriptionSize = '{{banner-description-size}}';\r\n            if (titleSize) {\r\n                titleEl.style.fontSize = titleSize;\r\n            }\r\n            if (descriptionSize) {\r\n                descriptionEl.style.fontSize = descriptionSize;\r\n            }\r\n        }\r\n        \r\n        function _applyBannerMargin(bannerSelector) {\r\n            var bannerEl = document.querySelector(bannerSelector);\r\n            var bannerTopMargin = '{{banner-top-margin}}' ? '{{banner-top-margin}}' : 0;\r\n            var bannerBottomMargin = '{{banner-bottom-margin}}' ? '{{banner-bottom-margin}}' : 0;\r\n            var additionalMarginToOriginalNavbar = 0;\r\n\r\n            if (bannerTopMargin) {\r\n                bannerEl.style.marginTop = parseInt(bannerTopMargin) + 'px';\r\n                additionalMarginToOriginalNavbar += parseInt(bannerTopMargin);\r\n            }\r\n            if (bannerBottomMargin) {\r\n                bannerEl.style.marginBottom = parseInt(bannerBottomMargin) + 'px';\r\n                additionalMarginToOriginalNavbar += parseInt(bannerBottomMargin);\r\n            }\r\n\r\n            \/\/ 5 is default margin for container\r\n            \/\/ 30 is navbar's height\r\n            var defaultContainerMargin = 5;\r\n            var navbarEl = document.querySelector('#header');\r\n            var navbarHeight = navbarEl.offsetHeight;\r\n\r\n            \/\/ adopt live section container\r\n            if (document.querySelector('html.live #container')) {\r\n                document.querySelector('html.live #container').style.top = (defaultContainerMargin + navbarHeight + parseInt(bannerEl.style.height) + parseInt(bannerTopMargin) + parseInt(bannerBottomMargin)) + 'px';\r\n            }\r\n        }\r\n        \r\n        function _applyCloseButtonColor(bannerSelector) {\r\n            var closeButtonColor = '{{banner-close-button-color}}';\r\n            var bannerEl = document.querySelector(bannerSelector);\r\n            var closeButtonEl = bannerEl.querySelector('.frosmo-leaderboard-banner-iframe-close');\r\n            \r\n            if (closeButtonColor) {\r\n                closeButtonEl.style.color = closeButtonColor;\r\n            }\r\n        }\r\n\r\n    }\r\n\r\n    function _getBannerSelector() {\r\n        var bannerSelector = '.frosmo-leaderboard-banner-iframe';\r\n        \r\n        if (_isLink()) {\r\n            bannerSelector = '.frosmo-leaderboard-banner-iframe-link';\r\n        }\r\n        \r\n        return bannerSelector;\r\n    }\r\n\r\n    function _isLink() {\r\n        return '{{{banner-link}}}';\r\n    }\r\n    \r\n    function _sendDataFromIframeToParent(eventName, content) {\r\n        var data = {\r\n            frosmo: true,\r\n            name: eventName,\r\n            content: content ? content : '',\r\n        }\r\n        var parentDomain = window.location.protocol + 'www.' + window.location.hostname.replace('als.', '');\r\n        window.parent.postMessage(data, parentDomain);\r\n    }\r\n\r\n\r\n    function _fixationHandling() {\r\n        var navbarEl = document.querySelector('#header');\r\n        var bannerEl = document.querySelector('.frosmo-leaderboard-banner-iframe-wrapper');\r\n        var bannerHeight = bannerEl.offsetHeight;\r\n\r\n        if (bannerHeight < window.scrollY) {\r\n            navbarEl.style.position = 'fixed';\r\n        } else {\r\n            navbarEl.style.position = 'relative';\r\n        }\r\n    }\r\n\r\n    function _getReferrerType() {\r\n        var link = '{{{banner-link}}}';\r\n\r\n        if (link.indexOf('\/promotions') > -1) {\r\n            return 'promotions';\r\n        }\r\n\r\n        if (link.indexOf('\/live_stream') > -1) {\r\n            return 'live-stream';\r\n        }\r\n\r\n        return 'normal';\r\n    }\r\n\r\n    function _addAuthorizationParameter(parameter) {\r\n        \/\/ sometimes (randomly) by undefined reason link is being\r\n        \/\/ added twice. trying to flush it before adding the parameter\r\n        var link = '{{{banner-link}}}';\r\n        if (!link) {\r\n            link = document.querySelector('.frosmo-leaderboard-banner-iframe-link > a').getAttribute('href');\r\n        }\r\n        if (link.indexOf('?') > -1) {\r\n            link += '&';\r\n        } else {\r\n            link += '?';\r\n        }\r\n        link += parameter; \r\n        document.querySelector('.frosmo-leaderboard-banner-iframe-link > a').setAttribute('href', link)\r\n    }\r\n<\/script>","dafa-273 - User Onboarding Guide":"<div class=\"frosmo-onboarding-guide-wrapper\">\r\n    <div class=\"frosmo-onboarding-guide-close\">\r\n        &times;\r\n    <\/div>\r\n    <div class=\"slider single-item\">\r\n        {{#images}}\r\n        <a class=\"frosmo-onboarding-guide-image\" href=\"{{link}}\" target=\"_blank\" style=\"background-image: url('{{.}}');\">\r\n            \r\n        <\/a>\r\n        {{\/images}}\r\n    <\/div>\r\n    <div class=\"frosmo-onboarding-guide-footer\">\r\n        \r\n    <\/div>\r\n<\/div>\r\n<div class=\"frosmo-onboarding-guide-background\"><\/div>\r\n<li class=\"pull-left frosmo-onboarding-guide