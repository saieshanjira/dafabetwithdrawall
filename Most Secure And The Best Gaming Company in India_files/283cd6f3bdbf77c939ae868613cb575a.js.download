        padding: 0;\r\n    }\r\n<\/style>\r\n\r\n<script type=\"text\/javascript\">\r\n    var instance = positionData.getMessageInstance();\r\n    \r\n    _fixPosition();\r\n    _setCustomSize();\r\n    _setCustomCloseButtonColor();\r\n    _centerPopup();\r\n    _formatLink();\r\n    _handlePopupEvents();\r\n\r\n    function _fixPosition() {\r\n        var body = document.querySelector('body');\r\n        body.classList.add('frosmo-fish-hunter-players');\r\n    }\r\n    function _setCustomSize() { \r\n        var customWidth = '{{width}}';\r\n        var customHeight = '{{height}}';\r\n        var innerEl = document.querySelector('.frosmo-fish-hunter-players-inner');\r\n\r\n        if (customWidth) {\r\n            innerEl.style.width = customWidth + 'px';\r\n        }\r\n\r\n        if (customHeight) {\r\n            innerEl.style.height = customHeight + 'px';\r\n        }\r\n    }\r\n\r\n    function _setCustomCloseButtonColor() {\r\n        var customcloseButtonColor = '{{closeButtonColor}}';\r\n        var closeButtonEl = document.querySelector('.frosmo-fish-hunter-players-close');\r\n\r\n        if (customcloseButtonColor) {\r\n            closeButtonEl.style.color('#' + customcloseButtonColor);\r\n        }\r\n    }\r\n\r\n    function _setCustomCloseButtonColor() {\r\n        var customcloseButtonColor = '{{closeButtonColor}}';\r\n        var closeButtonEl = document.querySelector('.frosmo-fish-hunter-players-close');\r\n\r\n        if (customcloseButtonColor) {\r\n            closeButtonEl.style.color('#' + customcloseButtonColor);\r\n        }\r\n    }\r\n    function _centerPopup() {\r\n        var wrapperEl = document.querySelector('.frosmo-fish-hunter-players-wrapper');\r\n        wrapperEl.style.top = Math.max(0, ((window.innerHeight - wrapperEl.offsetHeight) \/ 2)) + \"px\";\r\n        wrapperEl.style.left = Math.max(0, ((window.innerWidth - wrapperEl.offsetWidth) \/ 2)) + \"px\";\r\n    }\r\n\r\n    function _handlePopupEvents() {\r\n        var linkEl = document.querySelector('.frosmo-fish-hunter-players-link');\r\n        linkEl.addEventListener('click', function() {\r\n            instance.setClicked();\r\n        });\r\n\r\n        var closeButtonEl = document.querySelector('.frosmo-fish-hunter-players-close');\r\n        var bgEl = document.querySelector('.frosmo-fish-hunter-players-background');\r\n        var linkEl = document.querySelector('.frosmo-fish-hunter-players-link');\r\n        closeButtonEl.addEventListener('click', _closeHandler);\r\n        bgEl.addEventListener('click', _closeHandler);\r\n        linkEl.addEventListener('click', _closeHandler);\r\n\r\n        function _closeHandler() {\r\n            instance.setTrackable(false);\r\n            var body = document.querySelector('body');\r\n            var wrapperEl = document.querySelector('.frosmo-fish-hunter-players-wrapper');\r\n            var bgEl = document.querySelector('.frosmo-fish-hunter-players-background');\r\n\r\n            body.classList.remove('frosmo-fish-hunter-players');\r\n            wrapperEl.style.display = 'none';\r\n            bgEl.style.display = 'none';\r\n        }\r\n    }\r\n\r\n    function _formatLink() {\r\n        var link = '{{{url}}}';\r\n        \/\/ uncomment if you will need to add just token\r\n        \/\/ var token = _getToken();\r\n        \/\/ uncomment if you will need to add btag\r\n        \/\/ var btag = _getBtag();\r\n\r\n        var params = _getAllParams(_getFishHunterLink());\r\n        if (params) {\r\n            link += link.indexOf('?') > -1 ? '&' + params : '?' + params;\r\n        }\r\n        \/* if (token) {\r\n            link += link.indexOf('?') > -1 ? '&token=' + token : '?token=' + token;\r\n        }*\/\r\n\r\n        \/* if (btag) {\r\n            link += link.indexOf('?') > -1 ? '&btag=' + btag : '?btag=' + btag;\r\n        }*\/\r\n        var linkEl = document.querySelector('.frosmo-fish-hunter-players-link');\r\n        linkEl.setAttribute('href', link);\r\n    }\r\n\r\n    function _getToken() {\r\n        var linkEl = document.querySelector('nav #main-menu .main-menu-item > a');\r\n        var linkWithToken = linkEl.getAttribute('href');\r\n        var uri = frosmo.easy.utils.parseUri(linkWithToken);\r\n        return (uri.queryKey && uri.queryKey.token) ? uri.queryKey.token : '';\r\n    }\r\n\r\n    function _getFishHunterLink() {\r\n        var linkEls = document.querySelectorAll('nav #main-menu .main-menu-item > a');\r\n        var fishLink = frosmo.easy.utils.find(linkEls, function (linkEl) {\r\n            return linkEl.getAttribute('href').indexOf('fish-hunter') > -1;\r\n        });\r\n\r\n        return fishLink.getAttribute('href');\r\n    }\r\n\r\n    function _getAllParams(link) {\r\n        return frosmo.easy.utils.parseUri(link).query;\r\n    }\r\n\r\n    function _getBtag() {\r\n        var uri = frosmo.easy.utils.parseUri(window.location.href);\r\n        return (uri.queryKey && uri.queryKey.btag) ? uri.queryKey.btag : '';\r\n    }\r\n<\/script>","DAFA-1050 - Popup for dafa sports and next bet sports":"<div class=\"frosmo-first-deposit-wrapper\">\r\n    <div class=\"frosmo-first-deposit-inner\">\r\n        <button class=\"frosmo-first-deposit-close\">\u00d7<\/button>\r\n        <div class=\"frosmo-first-deposit-link\" >\r\n        <\/div>\r\n    <\/div>\r\n<\/div>\r\n<div class=\"frosmo-first-deposit-background\">\r\n<\/div>\r\n\r\n<style type=\"text\/css\">\r\n    body.frosmo-first-deposit {\r\n        overflow: hidden;\r\n    }\r\n    .frosmo-first-deposit-wrapper {\r\n        position: fixed;\r\n        z-index: 1002;\r\n    }\r\n    .frosmo-first-deposit-inner {\r\n        position: relative;\r\n        cursor: pointer;\r\n        width: 600px;\r\n        height: 300px;\r\n    }\r\n    .frosmo-first-deposit-link {\r\n        height: 100%;\r\n        width: 100%;\r\n        display: block;\r\n        background: #000;\r\n        background-image: url({{&image}});\r\n        background-position: center;\r\n       