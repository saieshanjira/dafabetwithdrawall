\n    }\r\n\r\n    function _setCustomBackground() {\r\n        var customBackgroundColor = '{{backgroundColor}}';\r\n    \r\n        if (customBackgroundColor) {\r\n            $('.frosmo-onboarding-guide-footer').css('background', customBackgroundColor);\r\n        }\r\n    }\r\n\r\n    function _setCoinsColor() {\r\n        var inactiveCoinColor = '{{inactiveCoinColor}}';\r\n        var activeCoinColor = '{{activeCoinColor}}';\r\n        var coinsRules = '';\r\n        if (inactiveCoinColor) {\r\n            coinsRules += '.frosmo-onboarding-guide-wrapper .slick-dots li button { background-color: ' + inactiveCoinColor + '; }';\r\n            coinsRules += '.frosmo-onboarding-guide-wrapper .slick-arrow { color: ' + inactiveCoinColor + '; }';\r\n        }\r\n\r\n        if (activeCoinColor) {\r\n            coinsRules += '.frosmo-onboarding-guide-wrapper .slick-dots li.slick-active button { background: ' + activeCoinColor + '; }';\r\n        }\r\n\r\n        if (coinsRules) {\r\n            var style = $('<style>' + coinsRules + '\\<\\\/style>');\r\n             $('html > head').append(style);\r\n        }\r\n    }\r\n    \r\n    function _centerPopup() {\r\n        $('.frosmo-onboarding-guide-wrapper').css('top', Math.max(0, (($(window).height() - $('.frosmo-onboarding-guide-wrapper').outerHeight()) \/ 2) + $(window).scrollTop()) + \"px\");\r\n        $('.frosmo-onboarding-guide-wrapper').css('left', Math.max(0, (($(window).width() - $('.frosmo-onboarding-guide-wrapper').outerWidth()) \/ 2) + $(window).scrollLeft()) + \"px\");\r\n    }\r\n        \r\n    function _handlePopupEvents() {\r\n        $('.frosmo-onboarding-guide-link').click(function() {\r\n            instance.setClicked();\r\n        });\r\n    \r\n        $('.frosmo-onboarding-guide-close, .frosmo-onboarding-guide-background, .frosmo-onboarding-guide-link').click(function() {\r\n            frosmo.easy.context.site.dafa1007wasPopupShown = true;\r\n            frosmo.easy.context.save();\r\n         